{
  "expo": {
    "name": "GeoTracking",
    "slug": "Geo-Tracking",
    "description": "high perfomance for your industry",
    "version": "1.0.20",
    "orientation": "portrait",
    "icon": "./assets/southernLogoVer2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/southern.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.elviscruz30.geotraking",
      "buildNumber": "3"
    },
    "android": {
      "package": "com.elviscruz30.geotraking",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/southernLogoVer2.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dcc3154d-485c-457a-8495-5126a1df9eb5"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/dcc3154d-485c-457a-8495-5126a1df9eb5"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
